{
    "manifest_version": 3,
    
    "name": "Web Embargo",
    "version": "1.0.0",
    "description": "Chrome extension to blacklist specific websites.",

    "action": {
        "default_title": "Web Embargo",
        "default_popup": "popup.html"
    },

    "background": {
        "service_worker": "background.js"
    },

    "permissions": [
        "storage",
        "tabs"
    ]
}